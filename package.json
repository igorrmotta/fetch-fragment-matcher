{
  "name": "fetch-fragment-matcher",
  "version": "0.1.5",
  "description": "Fetch a graphql endpoint to generate the fragmentTypes for apollo-client",
  "main": "src/index.js",
  "bin": {
    "get-fragment-matcher": "src/bin.js",
    "fetch-fragment-matcher": "src/bin.js"
  },
  "scripts": {
    "test-fetch": "node src/bin fetch-fragment-matcher --e http://localhost:8000/graphql --o ./output",
    "test-local": "node src/bin get-fragment-matcher --d ~/src/graphql --o ./output"
  },
  "keywords": [
    "apollo-client",
    "IntrospectionFragmentMatcher",
    "fragmentMatcher",
    "graphql",
    "fragments",
    "fragment-matcher",
    " heuristic fragment"
  ],
  "author": "igorrmotta",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "1.7.3",
    "yargs": "11.0.0",
    "graphql": "0.13.1",
    "graphql-tools": "2.21.0"
  },
  "repository": {
    "url": "https://github.com/igorrmotta/fetch-fragment-matcher"
  }
}